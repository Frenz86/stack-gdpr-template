# Example .env file for STAKC GDPR Template

# Project settings
PROJECT_NAME=STAKC GDPR Template
PROJECT_TEMPLATE=base
FRONTEND_TEMPLATE=nextjs_base
ENVIRONMENT=development
DEBUG=true

# Database
DATABASE_URL=postgresql://admin:secure123@localhost:5432/stakc_app
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20
DATABASE_ECHO=false

# Redis
REDIS_URL=redis://localhost:6379
REDIS_CACHE_TTL=3600

# Security
SECRET_KEY=CAMBIA_QUESTO_VALORE_IN_PRODUZIONE
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7
PASSWORD_MIN_LENGTH=8
ALLOWED_HOSTS=localhost,127.0.0.1
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# GDPR
GDPR_ENCRYPTION_KEY=CAMBIA_QUESTO_VALORE_IN_PRODUZIONE
GDPR_RETENTION_DAYS=1095
GDPR_CONSENT_EXPIRY_DAYS=365
GDPR_AUDIT_ENABLED=true
GDPR_AUTO_ANONYMIZE=true
GDPR_EXPORT_FORMAT=json

# Plugins (comma-separated)
ENABLED_PLUGINS=gdpr,security,analytics,audit
